<?xml version="1.0" encoding="UTF-8"?>
<SimulatorModelDescription
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  fmiVersion="2.0"
  modelName="simulator"
  description="Input data for a Simulator FMU"
  generationTool="SimulatorToFMU">
