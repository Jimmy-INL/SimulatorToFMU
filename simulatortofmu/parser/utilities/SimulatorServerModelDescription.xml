    <ScalarVariable
      name="_configurationFileName"
      description="parameter"
      causality="parameter"
	  start="C:\\Users\\Public\\server_config.txt"
	  type="String">
    </ScalarVariable>
	
    <ScalarVariable
      name="test"
      description="parameter"
      causality="parameter"
	  start="0.0"
	  type="Real">
    </ScalarVariable>
    <ScalarVariable
      name="input1"
      description="input"
      causality="input"
	  type="Real"
	  unit="A">
    </ScalarVariable>
    <ScalarVariable
      name="y"
      description="output"
      causality="output"
	  type="Real">
    </ScalarVariable>
  </ModelVariables>
</SimulatorModelDescription>
